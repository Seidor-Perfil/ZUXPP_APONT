<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		id="ID_TABLE_ADDFILTER_DIALOG"
		noDataText="{i18n>tableNoData}"
		title="{i18n>dialogTitle1}"
		search="onHandleSearchFilter"
		confirm="onHandleConfirmFilter"
		cancel="onHandleCloseFilter"
		multiSelect="true"
		items="{
			path : '/VHlpOrdemSet'
		}">
		<ColumnListItem>
			<cells>
				<ObjectIdentifier
					title="{Ordem}"
					text="{Descricao}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="{i18n>tableColumnLabel10}" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>